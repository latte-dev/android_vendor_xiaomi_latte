                        <WindowNormal1>120000</WindowNormal1>
                        <WindowNormal2>120000</WindowNormal2>
                        <WindowNormal3>120000</WindowNormal3>
                        <WindowWarning>120000</WindowWarning>
                        <WindowAlert>60000</WindowAlert>
